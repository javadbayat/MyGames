    function keyHandler()
    {
        switch (event.keyCode)
        {
        case 112 : //P
            if (paused)
                objects.pauseScreen.resume();
            else
                objects.pauseScreen.pause();
            break;
        case 13 : //Enter
            if (paused)
                objects.pauseScreen.resume();
            break;
        case 32 : //Space
            if ((!paused) && (objects.player.isHoldingShot))
                objects.shot.startMoving();
            break;
        }
    }

