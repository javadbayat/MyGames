            }
            else
            {
                gd("BrickShooter") = window;
                InfoMsgBox("This program has been added to the Global Dictionary with the key 'BrickShooter'.");
            }
        }
        catch (e)
        {
            alert("Unable to add the program to the global dictionary:\n" + e.message);
